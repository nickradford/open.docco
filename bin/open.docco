#!/usr/bin/env node

var program = require('commander');

function list(val) {
    return val.split(',');
}

program
    .version('0.0.0')
    .option('-r, --recursive', 'Recursively parse the file tree.')
    .option('-m, --maintain', 'Maintain the file structure in the output.')
    .option('-o, --output <files>', 'Output directory for the generated documentation.')
    .parse(process.ARGV);
    
console.log(program);